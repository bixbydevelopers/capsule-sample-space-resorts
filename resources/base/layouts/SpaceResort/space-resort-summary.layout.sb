<layout-macro-def id="space-resort-summary">
  <params>
    <param name="spaceResort" type="SpaceResort" min="Required" max="One"/>
  </params>
  <content>
    <layout-macro id="common:card">
      <title>{{spaceResort.name}}</title>
      <titleSize>large</titleSize>
      <image>{{#try}}{{spaceResort.imageSet.images[0].url}}{{/try}}</image>
      <imageType>background</imageType>
      <aspectRatio>2_1</aspectRatio>
    </layout-macro>
  </content>
  <style>
  </style>
</layout-macro-def>
