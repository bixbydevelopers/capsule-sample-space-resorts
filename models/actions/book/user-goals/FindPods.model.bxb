action (FindPods) {
  type (Search)
  collect {

    input (spaceResort) {
      type (SpaceResort)
      min (Required)
    }

    input (numberOfAstronauts) {
      type (NumberOfAstronauts)
      min (Required)
    }

    input (dateInterval) {
      type (DateInterval)
      min (Required)
    }
  }

  output (HabitatPod)
}