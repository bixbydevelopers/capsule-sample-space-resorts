action (PromptForDateInterval) {
  description (Prompt user to enter a date interval)
  type (Constructor)
  collect {
    computed-input (dateInterval) {
      type (DateInterval)
      min (Required)
      compute {
        intent {
          goal { @prompt-behavior(AlwaysElicitation) DateInterval }
        }
      }
    }
  }
  output (DateInterval) {
    evaluate { $expr(dateInterval) }
  }
}